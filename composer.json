{
  "name": "microgears/webstone-stdlib",
  "type": "library",
  "description": "SPL extensions, utilities & etc.",
  "keywords": [
    "microGears",
    "helpers",
    "stdlib"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Maxim Kirichenko",
      "email": "kirichenko.maxim@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*"
  },
  "autoload": {
    "psr-4": {"WebStone\\Stdlib\\": "src"}
